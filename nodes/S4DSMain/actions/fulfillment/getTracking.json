{
  "name": "getTracking",
  "method": "GET",
  "endpoint": "/fulfillment/tracking",
  "description": "Get tracking information with optional filters",
  "parameters": [
    {
      "name": "orderNumber",
      "type": "string",
      "required": false,
      "description": "Filter by order number",
      "in": "query"
    },
    {
      "name": "orderId",
      "type": "string",
      "required": false,
      "description": "Filter by order ID",
      "in": "query"
    },
    {
      "name": "orderStatus",
      "type": "string",
      "required": false,
      "description": "Filter by order status",
      "in": "query"
    },
    {
      "name": "campaign",
      "type": "string",
      "required": false,
      "description": "Filter by campaign",
      "in": "query"
    },
    {
      "name": "quantity",
      "type": "string",
      "required": false,
      "description": "Number of records to return",
      "in": "query",
      "default": "100"
    },
    {
      "name": "operationStartDate",
      "type": "string",
      "required": false,
      "description": "Start date for operation filter",
      "in": "query"
    },
    {
      "name": "operationEndDate",
      "type": "string",
      "required": false,
      "description": "End date for operation filter",
      "in": "query"
    }
  ],
  "requiresAuth": true,
  "response": {
    "type": "object",
    "properties": {
      "result": "array"
    }
  }
}