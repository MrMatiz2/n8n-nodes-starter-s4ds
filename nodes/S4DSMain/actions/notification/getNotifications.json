{
  "method": "GET",
  "endpoint": "/notifications",
  "description": "Get notifications with optional filters",
  "parameters": [
    {
      "name": "notificationTypeId",
      "type": "integer",
      "required": false,
      "description": "Filter by notification type ID",
      "in": "query"
    },
    {
      "name": "notificationTypeCode",
      "type": "string",
      "required": false,
      "description": "Filter by notification type code",
      "in": "query"
    },
    {
      "name": "contentType",
      "type": "string",
      "required": false,
      "description": "Filter by content type",
      "in": "query"
    },
    {
      "name": "quantity",
      "type": "integer",
      "required": false,
      "description": "Number of notifications to retrieve (default: 100)",
      "in": "query"
    }
  ],
  "requiresAuth": true,
  "response": {
    "type": "object",
    "properties": {
      "data": {
        "type": "array",
        "items": {
          "type": "object"
        }
      }
    }
  }
}