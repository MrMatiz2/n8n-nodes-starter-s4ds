{
  "method": "GET",
  "endpoint": "/division",
  "description": "Get territorial divisions with optional filters and sorting",
  "parameters": [
    {
      "name": "territorialCode",
      "type": "string",
      "required": false,
      "description": "Filter by territorial code",
      "in": "query"
    },
    {
      "name": "configurationCode",
      "type": "string",
      "required": false,
      "description": "Filter by configuration code",
      "in": "query"
    },
    {
      "name": "documentType",
      "type": "string",
      "required": false,
      "description": "Filter by document type",
      "in": "query"
    },
    {
      "name": "documentId",
      "type": "string",
      "required": false,
      "description": "Filter by document ID",
      "in": "query"
    },
    {
      "name": "territorialFatherCode",
      "type": "string",
      "required": false,
      "description": "Filter by territorial father code",
      "in": "query"
    },
    {
      "name": "orderBy",
      "type": "string",
      "required": false,
      "description": "Order results by field",
      "in": "query"
    },
    {
      "name": "quantity",
      "type": "integer",
      "required": false,
      "description": "Number of divisions to retrieve (default: 100)",
      "in": "query"
    }
  ],
  "requiresAuth": true,
  "response": {
    "type": "object",
    "properties": {
      "data": {
        "type": "array",
        "items": {
          "type": "object"
        }
      }
    }
  }
}