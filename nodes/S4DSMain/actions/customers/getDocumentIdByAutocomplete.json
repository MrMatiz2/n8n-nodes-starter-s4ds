{
  "name": "getDocumentIdByAutocomplete",
  "method": "GET",
  "endpoint": "/customer/search/{user}",
  "description": "Get document ID by autocomplete user search.",
  "parameters": [
    {
      "name": "user",
      "in": "path",
      "type": "string",
      "required": true,
      "description": "User search term"
    },
    {
      "name": "searchType",
      "in": "query",
      "type": "string",
      "required": false,
      "description": "Type of search to perform"
    },
    {
      "name": "consultantCode",
      "in": "query",
      "type": "string",
      "required": false,
      "description": "Consultant code filter"
    }
  ],
  "requiresAuth": true,
  "response": {
    "type": "object",
    "properties": {
      "results": {
        "type": "array",
        "items": {
          "type": "object"
        }
      }
    }
  }
}