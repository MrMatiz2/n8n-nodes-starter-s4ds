{
  "name": "getLanguages",
  "method": "GET",
  "endpoint": "/languages",
  "description": "Get languages with optional filters.",
  "parameters": [
    {
      "name": "languageId",
      "type": "integer",
      "required": false,
      "description": "Language ID to filter by",
      "in": "query"
    },
    {
      "name": "languageCode",
      "type": "string",
      "required": false,
      "description": "Language code to filter by",
      "in": "query"
    },
    {
      "name": "description",
      "type": "string",
      "required": false,
      "description": "Description to filter by",
      "in": "query"
    }
  ],
  "requiresAuth": true,
  "response": {
    "type": "array",
    "properties": {
      "items": "object"
    }
  }
}