{
  "name": "getCycles",
  "method": "GET",
  "endpoint": "/campaign/cycles",
  "description": "Get campaign cycles",
  "parameters": [
    {
      "name": "cycleId",
      "type": "integer",
      "required": false,
      "description": "Filter by cycle ID",
      "in": "query"
    },
    {
      "name": "cycleChannel",
      "type": "string",
      "required": false,
      "description": "Filter by cycle channel",
      "in": "query"
    },
    {
      "name": "cycleYear",
      "type": "string",
      "required": false,
      "description": "Filter by cycle year",
      "in": "query"
    },
    {
      "name": "quantity",
      "type": "integer",
      "required": false,
      "description": "Number of results to return (default: 100)",
      "in": "query"
    }
  ],
  "requiresAuth": true,
  "response": {
    "type": "object",
    "properties": {
      "cycles": "array"
    }
  }
}